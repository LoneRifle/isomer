{
  "name": "@isomerpages/isomer-components",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/isomerpages"
  },
  "version": "0.0.10",
  "type": "module",
  "description": "Isomer component library",
  "scripts": {
    "rollup": "rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publish-package": "rollup -c && npm run publish",
    "clean": "rimraf build",
    "build": "npm run clean && run-p build:*",
    "build:theme-types": "chakra-cli tokens ./src/theme/theme.ts",
    "build:main": "tsc -p tsconfig.build.json",
    "build:module": "tsc -p tsconfig.module.build.json"
  },
  "author": "isoengineers",
  "license": "ISC",
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@types/react": "^18.2.47",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "storybook": "^7.6.7",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/cjs",
    "dist/esm",
    "!**/*.spec.*",
    "!**/*.stories.*",
    "!**/*.json",
    "README.md"
  ],
  "types": "dist/cjs/index.d.ts",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@govtechsg/sgds-react": "^2.2.2",
    "@opengovsg/design-system-react": "^1.15.0",
    "@storybook/addon-viewport": "^7.6.7",
    "@vitejs/plugin-react": "^4.2.1",
    "react-icons": "^5.0.1",
    "vite": "^4.5.1"
  }
}
